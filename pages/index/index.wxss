}
.swiperItem{
  height: 50rpx;
}
.schoolItemCon{
  background: #f4f4f4;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.schoolList{
  margin: 0 30rpx 20rpx 30rpx;
  background: white;
  padding: 20rpx;
  border-radius: 20rpx;
}
.schoolList:last-child{
  margin-bottom: 0
}
.schoolLeft{
  float: left;
  width: 57%;
  padding-right: 3%;
  position: relative;
}
.schoolLeft>view{
  word-break:keep-all;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.schoolLeft>view:nth-child(1){
  font-size: 35rpx;
  font-family: "PingFang";
  color: rgb(0, 0, 0);
  font-weight: bold;
  line-height: 1.2;
}
.schoolLeft>view:nth-child(2){
  font-size: 25rpx;
  margin: 10rpx 0;
  font-weight: 400;
  color: #666666;
}
.schoolRight{
  float: left;
  width: 40%;
  height: 190rpx;
}
.schoolRight>image{
  width: 100%;
  height: 190rpx;
  border-radius: 20rpx;
}
.group3{
  float: left;
  width: 50%;
}
.group3>image{
  float: left;
  width: 25rpx;
  height: 30rpx;
  margin-top: 5rpx;
}
.group3>view:nth-child(2){
  float: left;
  font-size: 25rpx;
  padding: 0 10rpx;
  color: rgb(153, 153, 153);
  margin-top: 3rpx;
}
.group3>view:nth-child(3){
  font-size: 20rpx;
  float: left;
  color: rgb(153, 153, 153);
  font-family: "PingFang";
  margin-top: 9rpx;
}
.star{
  width: 40%;
  right: 0;
  float: right;
  /* margin-top: 10rpx; */
}
.star>image{
  float: right;
  width: 25rpx;
  height: 25rpx;
  margin-top: 7rpx;
}
.star>view:nth-child(1){
  padding-left: 10rpx;
  font-size: 25rpx;
  margin-top: 5rpx;
  float: right;
  font-family: "PingFang";
  color: rgb(153, 153, 153);
  line-height: 1.2;
}
.group4{
  margin-top: 15rpx;
}
.group4>view:nth-child(1){
  float: left;
  font-size: 30rpx;
  font-family: "PingFang";
  color: rgb(42, 185, 211);
  /* line-height: 1.2; */
}
.group4>view:nth-child(2){
  /* margin-top: 5rpx; */
  float: left;
  font-size: 20rpx;
  font-family: "PingFang";
  color: rgb(153, 153, 153);
  /* line-height: 1.4; */
  padding-left: 10rpx;
  text-decoration:line-through;
  margin-top: 10rpx;
}
.group4>view:nth-child(3){
  float: left;
  margin-left: 10rpx;
  margin-top: 11rpx;
  height: 0;
  width: 0;
  border-top:11rpx solid transparent;
  border-bottom:10rpx solid transparent;
  border-right:15rpx solid #f19e38;
}

.group4>view:nth-child(4){
  float: left;
  font-size: 18rpx;
  margin-top: 11rpx;
  font-family: "PingFang";
  color: rgb(255, 255, 255);
  background: #f19e38;
  padding: 2rpx;
  line-height: 1;
}
.group4>view:nth-child(5){
  float: right;
  font-size: 28rpx;
  font-family: "PingFang";
  color: rgb(153, 153, 153);
  line-height: 1.5;
}
