page{
  background: #f5f5f5;
}
.head{
  position: relative;
  background-image: '/images/icon/background.png';
}
.headicon{
  background: #32bfd0;
  height: 100rpx;
}
.headicon>image{
  width: 40rpx;
  height: 40rpx;
}
.headicon>image:nth-child(1){
  float: left;
  margin-left: 50rpx;
}
.headicon>image:nth-child(2),.headicon>image:nth-child(3){
  float: right;
  margin-right: 40rpx;
  height: 35rpx;
}
.headicon>image:nth-child(4){
  width: 110rpx;
  height: 110rpx;
  position: absolute;
  left: 320rpx;
  top: 30rpx;
}
.form{
  margin: 30rpx;
  font-size: 30rpx;
  line-height: 3;
  margin-top: 100rpx;
}
.name{
  font-size: 30rpx;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  background: white;
  
}
.name,.money{
  background: white;
  margin: 10rpx 0;
}
.tel,.qkCon{
  background: white;
}
.tel{
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
}
.qkCon{
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
}
.SchoolName>view:nth-child(1),.name>view:nth-child(1),.tel>view:nth-child(1),.money>view:nth-child(1){
  padding-left: 30rpx;
  float: left;
}
.SchoolName>input, .name>input, .tel>input, .money>input{
  float: left;
  padding-top: 20rpx;
}
.qkCon>view:nth-child(1){
  padding-left: 30rpx;
}
textarea{
  width: 630rpx;
  height: 150rpx;
  padding: 0 30rpx;

}
.submit{
  font-size: 30rpx;
  color: white;
  background: #f2c720;
  width: 100%;
  margin-top: 50rpx;
  border-radius: 20rpx;
  text-align: center;
}
.qkText{
  float: left;
  padding: 10rpx 20rpx;
  font-size: 30rpx;
  color: white;
  background: gray;
  border-radius: 20rpx;
  margin: 10rpx 15rpx;
  line-height: 1;
}
.seltext{
  background: rgb(47, 191, 207);
}